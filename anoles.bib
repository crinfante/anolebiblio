    doi = {10.1111/mec.13093},
    journal = {Mol Ecol},
    keywords = {anolis, archipelagos, biodiversity, change, climate, conservation, evolution, island, natural, parallel, phylogeography, selection, *file-import-16-02-09},
    month = mar,
    number = {5},
    pages = {1019--1030},
    posted-at = {2016-02-09 21:48:36},
    priority = {0},
    title = {{Widespread parallel population adaptation to climate variation across a radiation: implications for adaptation to climate change.}},
