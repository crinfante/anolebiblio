    citeulike-linkout-0 = {http://dx.doi.org/10.1002/cne.23823},
    doi = {10.1002/cne.23823},
    journal = {J Comp Neurol},
    keywords = {ab\_1143173, ab\_2110656, ab\_2155784, ab\_2160651, ab\_2195807, ab\_2239761, ab\_2314539, ab\_291611, ab\_528427, ab\_528490, ab\_528504, glia, glucagon, progenitor, retina, *file-import-16-02-09},
    month = jan,
