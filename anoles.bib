    journal = {Gen Comp Endocrinol},
    keywords = {*file-import-16-02-09},
    month = mar,
    number = {1},
    pages = {128--133},
    posted-at = {2016-02-09 21:48:32},
    priority = {0},
    title = {{Testosterone selectively affects aromatase and 5alpha-reductase activities in the green anole lizard brain.}},
    url = {http://dx.doi.org/10.1016/j.ygcen.2009.11.006},
