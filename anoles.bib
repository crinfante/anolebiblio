    doi = {10.1111/ele.12552},
    journal = {Ecol Lett},
    keywords = {activity, anolis, behaviour, change, ectotherm, global, physiology, temperature, *file-import-16-02-09},
    month = dec,
    posted-at = {2016-02-09 21:48:34},
