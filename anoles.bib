    pages = {R487--91},
    posted-at = {2016-02-09 21:48:23},
    priority = {0},
    title = {{Lactate and glucose metabolism in mouse (Mus musculus) and reptile (Anolis carolinensis) skeletal muscle.}},
    volume = {264},
    year = {1993}
}

