    month = apr,
    number = {3},
    pages = {223--227},
    posted-at = {2016-02-09 21:48:35},
    priority = {0},
    title = {{Immunocytochemical detection of beta-defensins and cathelicidins in the secretory granules of the tongue in the lizard Anolis carolinensis.}},
