    doi = {10.1002/ece3.1690},
    journal = {Ecol Evol},
    keywords = {dewlap, elasticity, evolution, signaling, *file-import-16-02-09},
    month = oct,
    number = {19},
    pages = {4400--4409},
    posted-at = {2016-02-09 21:48:34},
    priority = {0},
    title = {{The incredible shrinking dewlap: signal size, skin elasticity, and mechanical design in the green anole lizard (Anolis carolinensis).}},
