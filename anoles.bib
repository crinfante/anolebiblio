    citeulike-linkout-0 = {http://dx.doi.org/10.1111/evo.12328},
    doi = {10.1111/evo.12328},
    journal = {Evolution},
    keywords = {chromosome, cytogenetics, determination, homology, phylogeny, reptile, sex, x, *file-import-16-02-09},
    month = apr,
    number = {4},
    pages = {1027--1041},
    posted-at = {2016-02-09 21:48:37},
    priority = {0},
    title = {{Anolis sex chromosomes are derived from a single ancestral pair.}},
