    doi = {10.2108/zs150027},
    journal = {Zoolog Sci},
    keywords = {adaptation, anolis, behavior, dimorphisms, musculature, sexual, social, *file-import-16-02-09},
    month = oct,
    number = {5},
    pages = {438--446},
    posted-at = {2016-02-09 21:48:35},
    priority = {0},
    title = {{Sexual Dimorphisms of Appendicular Musculoskeletal Morphology Related to Social Displayin Cuban Anolis Lizards.}},
