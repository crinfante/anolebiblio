    doi = {10.1007/s00239-010-9427-y},
    journal = {J Mol Evol},
    keywords = {*file-import-16-02-09},
    month = mar,
    number = {3},
    pages = {265--273},
    posted-at = {2016-02-09 21:48:32},
    priority = {0},
    title = {{Deleterious mutations of a claw keratin in multiple taxa of reptiles.}},
