    keywords = {*file-import-16-02-09},
    month = mar,
    number = {6072},
    pages = {1086--1089},
    posted-at = {2016-02-09 21:48:30},
    priority = {0},
    title = {{Founder effects persist despite adaptive differentiation: a field experiment with lizards.}},
    url = {http://dx.doi.org/10.1126/science.1209566},
    volume = {335},
