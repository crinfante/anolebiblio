    doi = {10.1111/gcb.12253},
    journal = {Glob Chang Biol},
    keywords = {anolis, bay, cayos, cochinos, curve, islands, norops, performance, thermal, thermoregulation, *file-import-16-02-09},
    month = oct,
    number = {10},
    pages = {3093--3102},
    posted-at = {2016-02-09 21:48:38},
    priority = {0},
    title = {{The impact of climate change measured at relevant spatial scales: new hope for tropical lizards.}},
