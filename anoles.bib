    keywords = {*file-import-16-02-09},
    month = dec,
    number = {12},
    pages = {1030--1040},
    posted-at = {2016-02-09 21:48:27},
    priority = {0},
    title = {{Uterine motility in the reptile Anolis carolinensis: interactive effects of tension, prostaglandins, calcium, and vasotocin.}},
