    keywords = {*file-import-16-02-09},
    number = {2-4},
    pages = {94--111},
    posted-at = {2016-02-09 21:48:32},
    priority = {0},
    title = {{Transposable elements in reptilian and avian (sauropsida) genomes.}},
    url = {http://dx.doi.org/10.1159/000294999},
    volume = {127},
