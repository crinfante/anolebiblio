    citeulike-linkout-0 = {http://dx.doi.org/10.3382/ps.2012-02594},
    doi = {10.3382/ps.2012-02594},
    journal = {Poult Sci},
    keywords = {*file-import-16-02-09},
    month = jan,
    number = {1},
    pages = {90--96},
    posted-at = {2016-02-09 21:48:38},
    priority = {0},
    title = {{Expressed sequence tag analysis of the emu (Dromaius novaehollandiae) pituitary by 454 GS Junior pyrosequencing.}},
