    doi = {10.1111/evo.12357},
    journal = {Evolution},
    keywords = {adaptive, anolis, chromosomes, determination, radiation, reptiles, sceloporus, sex, *file-import-16-02-09},
    month = jul,
    number = {7},
    pages = {2079--2085},
    posted-at = {2016-02-09 21:48:37},
