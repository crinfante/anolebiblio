    doi = {10.1111/joa.12394},
    journal = {J Anat},
    keywords = {biting, dimorphism, head, muscle, sexual, shape, *file-import-16-02-09},
    month = jan,
    number = {1},
    pages = {85--95},
    posted-at = {2016-02-09 21:48:35},
    priority = {0},
    title = {{Proximate determinants of bite force in Anolis lizards.}},
