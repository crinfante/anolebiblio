    keywords = {*file-import-16-02-09},
    month = jun,
    number = {2},
    pages = {185--189},
    posted-at = {2016-02-09 21:48:35},
    priority = {0},
    title = {{A new species of Eimeria Schneider, 1875 (Apicomplexa: Eimeriidae) from the brown anole Anolis sagrei Dumeril \& Bibron (Sauria: Dactyloidae) in Florida, USA.}},
