    citeulike-linkout-0 = {http://dx.doi.org/10.1186/1471-2164-14-49},
    doi = {10.1186/1471-2164-14-49},
    journal = {BMC Genomics},
    keywords = {*file-import-16-02-09},
    pages = {49},
    posted-at = {2016-02-09 21:48:38},
    priority = {0},
    title = {{Genome reannotation of the lizard Anolis carolinensis based on 14 adult and embryonic deep transcriptomes.}},
    url = {http://dx.doi.org/10.1186/1471-2164-14-49},
    volume = {14},
    year = {2013}
}

@article{Ng:2013ab,
    abstract = {{Although the importance of signals involved in species recognition and sexual selection to speciation is widely recognized, the processes that underlie signal divergence are still a matter of debate. Several possible processes have been hypothesized, including genetic drift, arbitrary sexual selection, and adaptation to local signaling environments. We use comparative analyses to investigate whether the remarkable geographic variation of dewlap phenotype in a Hispaniolan trunk Anolis lizard (A. distichus) is a result of adaptive signal divergence to heterogeneous environments. We recover a repeated pattern of divergence in A. distichus dewlap color, pattern, and size with environmental variation across Hispaniola. These results are aligned with ecological models of signal divergence and provide strong evidence for dewlap adaptation to local signaling environments. We also find that A. distichus dewlaps vary with the environment in a different manner to other previously studied anoles, thus expanding upon previous predictions on the direction dewlaps will diverge in perceptual color space in response to the environment.}},
    address = {Department of Biology, University of Rochester, Rochester, New York 14627, USA. julienne.ng@rochester.edu},
    author = {Ng, Julienne and Landeen, Emily L. and Logsdon, Ryane M. and Glor, Richard E.},
