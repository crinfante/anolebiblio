    citeulike-linkout-0 = {http://dx.doi.org/10.1016/j.dci.2013.07.002},
    doi = {10.1016/j.dci.2013.07.002},
    journal = {Dev Comp Immunol},
    keywords = {ifn-gamma, ifng, ifngr1, ifngr2, lizard, *file-import-16-02-09},
