    month = feb,
    number = {2},
    pages = {293--305},
    posted-at = {2016-02-09 21:48:33},
    priority = {0},
    title = {{What determines dewlap diversity in Anolis lizards? An among-island comparison.}},
    url = {http://dx.doi.org/10.1111/j.1420-9101.2008.01643.x},
    volume = {22},
