    doi = {10.1016/j.cbpa.2009.02.009},
    journal = {Comp Biochem Physiol A Mol Integr Physiol},
    keywords = {*file-import-16-02-09},
    month = jun,
    number = {2},
    pages = {181--184},
    posted-at = {2016-02-09 21:48:33},
    priority = {0},
    title = {{The influence of metabolic heat production on body temperature of a small lizard, Anolis carolinensis.}},
