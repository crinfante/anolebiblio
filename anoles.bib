    journal = {Eur J Neurosci},
    keywords = {*file-import-16-02-09},
    month = jan,
    number = {1},
    pages = {118--129},
    posted-at = {2016-02-09 21:48:38},
    priority = {0},
    title = {{Immunohistological and electrophysiological evidence that N-acetylaspartylglutamate is a co-transmitter at the vertebrate neuromuscular junction.}},
    url = {http://dx.doi.org/10.1111/ejn.12027},
    volume = {37},
