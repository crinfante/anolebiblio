    journal = {Mol Phylogenet Evol},
    keywords = {delimitation, miocene, norops, phylogeography, savanna, speciation, species, *file-import-16-02-09},
    month = jan,
    number = {Pt A},
    pages = {279--289},
    posted-at = {2016-02-09 21:48:35},
    priority = {0},
    title = {{Cryptic lineages and diversification of an endemic anole lizard (Squamata, Dactyloidae) of the Cerrado hotspot.}},
    url = {http://dx.doi.org/10.1016/j.ympev.2015.09.005},
    volume = {94},
