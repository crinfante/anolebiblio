    posted-at = {2016-02-09 21:48:36},
    priority = {0},
    title = {{Variation in the position of the jugal medial ridge among lizards (reptilia: squamata): its functional and taxonomic significance.}},
    url = {http://dx.doi.org/10.1002/ar.22989},
    volume = {297},
