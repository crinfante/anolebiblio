    journal = {G3 (Bethesda)},
    keywords = {chromosomes, determination, dosage, gene, genetics, lizard, of, qpcr, sex, *file-import-16-02-09},
    month = nov,
    number = {11},
    pages = {2107--2113},
    posted-at = {2016-02-09 21:48:36},
