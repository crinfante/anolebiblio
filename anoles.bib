    doi = {10.1016/j.ygcen.2013.04.007},
    journal = {Gen Comp Endocrinol},
    keywords = {*file-import-16-02-09},
    month = jul,
    pages = {75--84},
    posted-at = {2016-02-09 21:48:38},
    priority = {0},
    title = {{Evolution of the melanocortin-2 receptor in tetrapods: studies on Xenopus tropicalis MC2R and Anolis carolinensis MC2R.}},
