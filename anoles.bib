    citeulike-article-id = {13930012},
    citeulike-linkout-0 = {http://dx.doi.org/10.1073/pnas.1506516112},
    doi = {10.1073/pnas.1506516112},
    journal = {Proc Natl Acad Sci U S A},
    keywords = {adaptive, anolis, ecomorph, hispaniola, miocene, radiation, *file-import-16-02-09},
    month = aug,
    number = {32},
    pages = {9961--9966},
