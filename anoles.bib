    pages = {149--155},
    posted-at = {2016-02-09 21:48:31},
    priority = {0},
    title = {{Does solitary incubation enhance egg water uptake and offspring quality in a lizard that produces single-egg clutches?}},
    url = {http://dx.doi.org/10.1002/jez.660},
    volume = {315},
    year = {2011}
