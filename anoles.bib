    citeulike-article-id = {13930111},
    citeulike-linkout-0 = {http://dx.doi.org/10.1111/evo.12184},
    doi = {10.1111/evo.12184},
    journal = {Evolution},
    keywords = {adaptive, contingency, convergence, genetics, landscape, radiation, recurrence, spatial, speciation, *file-import-16-02-09},
    month = nov,
    number = {11},
    pages = {3175--3190},
    posted-at = {2016-02-09 21:48:37},
    priority = {0},
    title = {{Determinism in the diversification of Hispaniolan trunk-ground anoles (Anolis cybotes species complex).}},
    url = {http://dx.doi.org/10.1111/evo.12184},
