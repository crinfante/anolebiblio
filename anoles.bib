    doi = {10.1016/j.zool.2006.06.001},
    journal = {Zoology (Jena)},
    keywords = {*file-import-16-02-09},
    number = {1},
    pages = {2--8},
    posted-at = {2016-02-09 21:48:27},
    priority = {0},
    title = {{Microhabitat use, diet, and performance data on the Hispaniolan twig anole, Anolis sheplani: pushing the boundaries of morphospace.}},
