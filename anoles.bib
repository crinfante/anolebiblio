    doi = {10.1098/rsbl.2013.1093},
    journal = {Biol Lett},
    keywords = {anolis, heterogamety, lizard, male, molecular, pleurodonta, qpcr, sexing, *file-import-16-02-09},
    month = mar,
    number = {3},
    pages = {20131093},
