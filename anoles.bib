    citeulike-article-id = {13929599},
    citeulike-linkout-0 = {http://dx.doi.org/10.1073/pnas.0606204104},
    doi = {10.1073/pnas.0606204104},
    journal = {Proc Natl Acad Sci U S A},
    keywords = {*file-import-16-02-09},
    month = feb,
    number = {8},
    pages = {2767--2772},
    posted-at = {2016-02-09 21:48:26},
    priority = {0},
    title = {{Phylogenomics of nonavian reptiles and the structure of the ancestral amniote genome.}},
