    doi = {10.1007/s00359-009-0478-8},
    journal = {J Comp Physiol A Neuroethol Sens Neural Behav Physiol},
    keywords = {*file-import-16-02-09},
    month = nov,
    number = {11},
    pages = {1043--1060},
    posted-at = {2016-02-09 21:48:33},
